{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/tienda/index.html"
    },
    {
      "src": "^/tienda$",
      "dest": "/tienda/index.html"
    },
    {
      "src": "^/tienda/(.*)$",
      "dest": "/tienda/$1"
    },
    {
      "src": "^/dashboard$",
      "dest": "/dashboard/index.html"
    },
    {
      "src": "^/dashboard/(.*)$",
      "dest": "/dashboard/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}