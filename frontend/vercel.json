{
  "version": 2,
  "builds": [
    {
      "src": "tienda/**",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^(.*)$",
      "dest": "/tienda/$1"
    },
        {
      "src": "^/dashboard/(.*)$",
      "dest": "/dashboard/$1"
    }
  ]
}